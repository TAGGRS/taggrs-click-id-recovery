/**
 * Google Ads Click ID Cookie Manager
 * Server-side GTM tag for managing Google Ads tracking cookies
 * Extracts gclid, gbraid, and wbraid parameters from URLs and sets appropriate cookies
 */

// Required APIs
const getAllEventData = require("getAllEventData");
const decodeUriComponent = require("decodeUriComponent");
const queryPermission = require("queryPermission");
const setCookie = require("setCookie");
const logToConsole = require("logToConsole");
const getTimestampMillis = require("getTimestampMillis");
const getCookieValues = require("getCookieValues");
const getType = require("getType");
const makeString = require("makeString");
const parseUrl = require("parseUrl");

// Configuration constants with defaults
const PARAM_GCLID = data.param_gclid || "gclid";
const PARAM_GBRAID = data.param_gbraid || "gbraid";
const PARAM_WBRAID = data.param_wbraid || "wbraid";
const COOKIE_DOMAIN = data.cookie_domain || "";
const REQUIRE_CONSENT = data.require_consent === true;
const BACKUP_COOKIES = data.backup_cookies === true;
const DEBUG_MODE = data.debug_mode === true;

// Cookie configuration
const COOKIE_MAX_AGE = 60 * 60 * 24 * 90; // 90 days in seconds

function debugLog(message, value) {
  if (DEBUG_MODE) {
    if (value !== undefined) {
      logToConsole(message, value);
    } else {
      logToConsole(message);
    }
  }
}

function isValidClickId(value) {
  if (getType(value) !== "string") {
    return false;
  }

  if (!value) {
    return false;
  }

  return true;
}

function getParamFromUrl(url, paramName) {
  if (
    getType(url) !== "string" ||
    !url ||
    getType(paramName) !== "string" ||
    !paramName
  ) {
    return undefined;
  }

  const urlObject = parseUrl(url);
  if (!urlObject || !urlObject.searchParams) {
    return undefined;
  }

  return urlObject.searchParams[paramName];
}

function safeSetCookie(name, value) {
  if (
    getType(name) !== "string" ||
    !name ||
    getType(value) !== "string" ||
    !value
  ) {
    debugLog("Invalid cookie name or value:", name + "=" + makeString(value));
    return false;
  }

  const options = {
    path: "/",
    secure: true,
    sameSite: "Lax",
    "max-age": COOKIE_MAX_AGE,
  };

  // Add domain if specified
  if (COOKIE_DOMAIN) {
    options.domain = COOKIE_DOMAIN;
  }

  if (queryPermission("set_cookies", name, options)) {
    setCookie(name, value, options);
    debugLog("Cookie set successfully:", name);
    return true;
  } else {
    debugLog("Permission denied for cookie:", name);
    return false;
  }
}

function formatCookieValue(prefix, clickId, timestamp) {
  return prefix + "." + makeString(timestamp) + "." + clickId;
}

function formatFpgclawValue(gclid, timestamp) {
  return "2.1.k" + gclid + "$1" + makeString(timestamp);
}

function setGclidCookies(gclid, timestamp) {
  if (!isValidClickId(gclid)) {
    debugLog("Invalid GCLID format:", gclid);
    return;
  }

  const gclValue = formatCookieValue("GCL", gclid, timestamp);
  const fpgclawValue = formatFpgclawValue(gclid, timestamp);

  if (BACKUP_COOKIES) {
    // Set backup cookies first
    safeSetCookie("_taggrs_aw", gclValue);
    safeSetCookie("_taggrs_fpgcl_aw", gclValue);
    safeSetCookie("_taggrs_FPGCLAW", fpgclawValue);

    // Copy to primary cookies
    const backupAwValues = getCookieValues("_taggrs_aw");
    const backupFpgclValues = getCookieValues("_taggrs_fpgcl_aw");
    const backupFpgclawValues = getCookieValues("_taggrs_FPGCLAW");

    if (backupAwValues && backupAwValues[0]) {
      safeSetCookie("_gcl_aw", backupAwValues[0]);
    }
    if (backupFpgclValues && backupFpgclValues[0]) {
      safeSetCookie("_fpgcl_aw", backupFpgclValues[0]);
    }
    if (backupFpgclawValues && backupFpgclawValues[0]) {
      safeSetCookie("FPGCLAW", backupFpgclawValues[0]);
    }
  } else {
    // Set primary cookies directly
    safeSetCookie("_gcl_aw", gclValue);
    safeSetCookie("_fpgcl_aw", gclValue);
    safeSetCookie("FPGCLAW", fpgclawValue);
  }
}

function setGbraidCookies(gbraid, timestamp) {
  if (!isValidClickId(gbraid)) {
    debugLog("Invalid GBRAID format:", gbraid);
    return;
  }

  const gclValue = formatCookieValue("GCL", gbraid, timestamp);

  if (BACKUP_COOKIES) {
    safeSetCookie("_taggrs_gb", gclValue);

    const backupValues = getCookieValues("_taggrs_gb");
    if (backupValues && backupValues[0]) {
      safeSetCookie("_gcl_gb", backupValues[0]);
    }
  } else {
    safeSetCookie("_gcl_gb", gclValue);
  }
}

function setWbraidCookies(wbraid, timestamp) {
  if (!isValidClickId(wbraid)) {
    debugLog("Invalid WBRAID format:", wbraid);
    return;
  }

  const gclValue = formatCookieValue("GCL", wbraid, timestamp);

  if (BACKUP_COOKIES) {
    safeSetCookie("_taggrs_wb", gclValue);

    const backupValues = getCookieValues("_taggrs_wb");
    if (backupValues && backupValues[0]) {
      safeSetCookie("_gcl_wb", backupValues[0]);
    }
  } else {
    safeSetCookie("_gcl_wb", gclValue);
  }
}

// Main execution
debugLog("Starting Google Ads Click ID processing");

// Get event data
const eventData = getAllEventData();
if (!eventData) {
  debugLog("No event data available");
  data.gtmOnSuccess();
  return;
}

// Check consent if required
if (REQUIRE_CONSENT) {
  const consent = eventData.consent;
  if (!consent || consent.ad_storage !== "granted") {
    debugLog("Ad storage consent not granted");
    data.gtmOnSuccess();
    return;
  }
}

// Extract page location
const pageLocation = eventData.page_location || eventData.dl || "";
if (!pageLocation) {
  debugLog("No page location found in event data");
  data.gtmOnSuccess();
  return;
}

debugLog("Processing URL:", pageLocation);

// Extract click IDs from URL
const gclid = getParamFromUrl(pageLocation, PARAM_GCLID);
const gbraid = getParamFromUrl(pageLocation, PARAM_GBRAID);
const wbraid = getParamFromUrl(pageLocation, PARAM_WBRAID);

debugLog("Extracted GCLID:", gclid);
debugLog("Extracted GBRAID:", gbraid);
debugLog("Extracted WBRAID:", wbraid);

// Get current timestamp
const timestamp = getTimestampMillis();

// Set cookies for each found click ID
if (gclid) {
  setGclidCookies(gclid, timestamp);
}

if (gbraid) {
  setGbraidCookies(gbraid, timestamp);
}

if (wbraid) {
  setWbraidCookies(wbraid, timestamp);
}

debugLog("Cookie processing completed");
data.gtmOnSuccess();


